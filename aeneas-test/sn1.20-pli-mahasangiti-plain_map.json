   "id": "sn1.20:22.1",
   "language": "eng",
   "lines": [
    "\u201cNa khvaham, bhante, imassa bhagavata sankhittena bhasitassa vittharena attham ajanami."
   ]
  },
  {
   "begin": "795.040",
   "children": [],
   "end": "816.840",
   "id": "sn1.20:22.2",
   "language": "eng",
   "lines": [
    "Sadhu me, bhante, bhagava tatha bhasatu yathaham imassa bhagavata sankhittena bhasitassa vittharena attham janeyyan\u201dti."
   ]
  },
  {
   "begin": "816.840",
   "children": [],
   "end": "822.920",
   "id": "sn1.20:23.1",
   "language": "eng",
   "lines": [
    "\u201cSamo visesi uda va nihino,"
   ]
  },
  {
   "begin": "822.920",
   "children": [],
   "end": "826.120",
   "id": "sn1.20:23.2",
   "language": "eng",
   "lines": [
    "Yo ma\u00f1\u00f1ati so vivadetha tena;"
   ]
  },
  {
   "begin": "826.120",
   "children": [],
   "end": "832.000",
   "id": "sn1.20:23.3",
   "language": "eng",
   "lines": [
    "Tisu vidhasu avikampamano,"
   ]
  },
  {
   "begin": "832.000",
   "children": [],
   "end": "841.280",
   "id": "sn1.20:23.4",
   "language": "eng",
   "lines": [
    "Samo visesiti na tassa hoti;"
   ]
  },
  {
   "begin": "841.280",
   "children": [],
   "end": "846.440",
   "id": "sn1.20:23.5",
   "language": "eng",
   "lines": [
    "Sace vijanasi vadehi yakkha\u201dti."
   ]
  },
  {
   "begin": "846.440",
   "children": [],
   "end": "860.720",
   "id": "sn1.20:24.1",
   "language": "eng",
   "lines": [
    "\u201cImassapi khvaham, bhante, bhagavata sankhittena bhasitassa na vittharena attham ajanami."
   ]
  },
  {
   "begin": "860.720",
   "children": [],
   "end": "874.120",
   "id": "sn1.20:24.2",
   "language": "eng",
   "lines": [
    "Sadhu me, bhante, bhagava tatha bhasatu yathaham imassa bhagavata sankhittena bhasitassa vittharena attham janeyyan\u201dti."
   ]
  },
  {
   "begin": "874.120",
   "children": [],
   "end": "884.520",
   "id": "sn1.20:25.1",
   "language": "eng",
   "lines": [
    "\u201cPahasi sankham na vimanamajjhaga,"
   ]
  },
  {
   "begin": "884.520",
   "children": [],
   "end": "886.640",
   "id": "sn1.20:25.2",
   "language": "eng",
   "lines": [
    "Acchecchi tanham idha namarupe;"
   ]
  },
  {
   "begin": "886.640",
   "children": [],
   "end": "893.200",
   "id": "sn1.20:25.3",
   "language": "eng",
   "lines": [
    "Tam chinnagantham anigham nirasam,"
   ]
  },
  {
   "begin": "893.200",
   "children": [],
   "end": "899.040",
   "id": "sn1.20:25.4",
   "language": "eng",
   "lines": [
    "Pariyesamana najjhagamum;"
   ]
  },
  {
   "begin": "899.040",
   "children": [],
   "end": "904.640",
   "id": "sn1.20:25.5",
   "language": "eng",
   "lines": [
    "Deva manussa idha va huram va,"
   ]
  },
  {
   "begin": "904.640",
   "children": [],
   "end": "913.000",
   "id": "sn1.20:25.6",
   "language": "eng",
   "lines": [
    "Saggesu va sabbanivesanesu."
   ]
  },
  {
   "begin": "913.000",
   "children": [],
   "end": "922.120",
   "id": "sn1.20:26.1",
   "language": "eng",
   "lines": [
    "Sace vijanasi vadehi yakkha\u201dti."
   ]
  },
  {
   "begin": "922.120",
   "children": [],
   "end": "931.840",
   "id": "sn1.20:27.1",
   "language": "eng",
   "lines": [
    "\u201cImassa khvaham, bhante, bhagavata sankhittena bhasitassa evam vittharena attham ajanami\u2014"
   ]
  },
  {
   "begin": "931.840",
   "children": [],
   "end": "939.080",
   "id": "sn1.20:28.1",
   "language": "eng",
   "lines": [
    "Papam na kayira vacasa manasa,"
   ]
  },
  {
   "begin": "939.080",
   "children": [],
   "end": "944.200",
   "id": "sn1.20:28.2",
   "language": "eng",
   "lines": [
    "Kayena va ki\u00f1cana sabbaloke;"
   ]
  },
  {
   "begin": "944.200",
   "children": [],
   "end": "954.160",
   "id": "sn1.20:28.3",
   "language": "eng",
   "lines": [
    "Kame pahaya satima sampajano,"
   ]
  },
  {
   "begin": "954.160",
   "children": [],
   "end": "965.360",
   "id": "sn1.20:28.4",
   "language": "eng",
   "lines": [
    "Dukkham na sevetha anatthasamhitan\u201dti."
   ]
  },
  {
   "begin": "965.360",
   "children": [],
   "end": "967.400",
   "id": "sn1.20:28.5",
   "language": "eng",
   "lines": [
    "Nandanavaggo dutiyo."
   ]
  },
  {
   "begin": "967.400",
   "children": [],
   "end": "972.160",
   "id": "sn1.20:29.1",
   "language": "eng",
   "lines": [
    "Nandana nandati ceva,"
   ]
  },
  {
   "begin": "972.160",
   "children": [],
   "end": "976.640",
   "id": "sn1.20:29.2",
   "language": "eng",
   "lines": [
    "Natthiputtasamena ca;"
   ]
  },
  {
   "begin": "976.640",
   "children": [],
   "end": "980.840",
   "id": "sn1.20:29.3",
   "language": "eng",
   "lines": [
    "Khattiyo sanamano ca,"
   ]
  },
  {
   "begin": "980.840",
   "children": [],
   "end": "986.320",
   "id": "sn1.20:29.4",
   "language": "eng",
   "lines": [
    "Niddatandi ca dukkaram;"
   ]
  },
  {
   "begin": "986.320",
   "children": [],
   "end": "989.320",
   "id": "sn1.20:29.5",
   "language": "eng",
   "lines": [
    "Hiri kutika navamo,"
   ]
  },
  {
   "begin": "989.320",
   "children": [],
   "end": "999.040",
   "id": "sn1.20:29.6",
   "language": "eng",
   "lines": [
    "Dasamo vutto samiddhinati."
   ]
  }
 ]
}